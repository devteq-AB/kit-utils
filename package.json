{
	"name": "@devteq/kit-utils",
	"version": "0.0.2",
	"license": "MIT",
	"description": "Collection of utilities for SvelteKit apps",
	"author": {
		"name": "Jonas Musekura Lundsholm",
		"email": "jonas@lundsholm.se"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/devteq-AB/kit-utils"
	},
	"bugs": {
		"url": "https://github.com/devteq-AB/kit-utils/issues"
	},
	"homepage": "https://github.com/devteq-AB/kit-utils#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"release": "npx bumpp --commit --tag --push && npm run package && cd ./package && npm publish"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@unocss/reset": "^0.45.5",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.13",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"unocss": "^0.45.5",
		"vite": "^3.0.0"
	},
	"keywords": [
		"svelte-kit",
		"SvelteKit",
		"front-end",
		"utilties",
		"typescript"
	],
	"type": "module"
}
